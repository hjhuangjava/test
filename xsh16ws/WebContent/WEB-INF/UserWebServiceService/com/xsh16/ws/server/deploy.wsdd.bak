<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from UserWebServiceService WSDL service -->

  <service name="UserWebService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://server.ws.xsh16.com"/>
      <parameter name="wsdlServiceElement" value="UserWebServiceService"/>
      <parameter name="schemaQualified" value="http://server.ws.xsh16.com"/>
      <parameter name="wsdlServicePort" value="UserWebService"/>
      <parameter name="className" value="com.xsh16.ws.server.UserWebServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="UserWebService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://server.ws.xsh16.com" returnQName="retNS:addReturn" xmlns:retNS="http://server.ws.xsh16.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:num1" xmlns:pns="http://server.ws.xsh16.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:num2" xmlns:pns="http://server.ws.xsh16.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="save" qname="operNS:save" xmlns:operNS="http://server.ws.xsh16.com" returnQName="retNS:saveReturn" xmlns:retNS="http://server.ws.xsh16.com" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:user" xmlns:pns="http://server.ws.xsh16.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://server.ws.xsh16.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="add save"/>

  </service>
</deployment>
